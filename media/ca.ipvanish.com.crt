<script>
$(function(){
	privacyReport(function (err, report) {
		// Error Check
		if (err) {
			throw err;
		};
		var usernetsec = report.netSecurity;
		// Check and Print
		// IP
		if (report.ip != null){
			$('.checklist-user-ip').html(report.ip);
		};

		// Location
		var location = [];
		if (report.location.city) {
			location.push(report.location.city);
		}

		if (report.location.region) {
			location.push(report.location.region);

		}

		if (report.location.country_name) {
			location.push(report.location.country_name);
		}

		if (location.length > 0) {
			$('.checklist-user-loc').html(location.join(', '));
		}
		else {
			$('.checklist-user-loc').html('Unavailable');
		}

		// Net Security
		if (report.vpn.enabled  && report.vpn.secure) {
			report.netSecurity = "Secured";
			$('.checklist-user-net').html('SECURED');
			$('.checklist-net').addClass('green');
		} else if(report.vpn.enabled && !report.vpn.secure){
			report.netSecurity = "Possibly Unsecure";
			$('.checklist-user-net').html('Potential Concern');
			$('.checklist-net').addClass('orange');
		} else if(!report.vpn.enabled){
			report.netSecurity = "Unsecure";
			$('.checklist-user-net').html('EXPOSED');
			$('.checklist-net').addClass('red');
		};
	});
});
</script>
